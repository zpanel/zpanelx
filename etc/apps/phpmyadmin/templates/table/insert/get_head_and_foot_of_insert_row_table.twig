<div class="table-responsive-lg">
  <table class="table table-light table-striped align-middle my-3 insertRowTable w-auto">
    <thead>
    <tr>
      <th>{% trans 'Column' %}</th>
      {{ type|raw }}
      {{ function|raw }}
      <th>{% trans 'Null' %}</th>
      <th class="w-50">{% trans 'Value' %}</th>
    </tr>
    </thead>
     <tfoot>
